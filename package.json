    "bower": "^1.8.0"
  },
  "private": false,
  "main": "oauth2-scope-selector.html",
  "scripts": {
    "lint": "polymer lint oauth2-scope-selector.html",
    "test-sauce": "polymer test --plugin sauce --job-name \"oauth2-scope-selector:local-test\"",
